<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ebaycatalogsearch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/shopping_icon_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/shopping_icon_round_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Details_page"></activity>
        <activity android:name=".SearchResults" />
        <activity android:name=".MainActivity" android:theme="@style/AppTheme.Launcher" android:label="eBay Catalog Search">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>